<Window x:Class="SongRequestDesktopV2Rewrite.Authentication"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SongRequestDesktopV2Rewrite"
        xmlns:cef="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
        mc:Ignorable="d"
        Title="Authentication - TechnikAG SongRequest"
        Width="800" Height="450"
        WindowStartupLocation="CenterScreen"
        Background="Black"
        Topmost="True" Icon="/SRshortLogo.png">
    <Grid>
        <cef:ChromiumWebBrowser Name="Browser"
                                Address="https://accounts.google.com/ServiceLogin?service=youtube"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch" />
    </Grid>
</Window>
